{
  "expo": {
    "name": "Crechespots",
    "slug": "crechespots-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "crechespots",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "4fzdc2evr5.skadnetwork"
          }
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "crechespots"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "https",
          "crechespots"
        ]
      },
      "bundleIdentifier": "com.softglitchsolutions.crechespots"
    },
    "android": {
      "package": "com.softglitchsolutions.crechespots",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "crechespots",
              "host": "*"
            },
            {
              "scheme": "crechespots",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/app-icon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "userInterfaceStyle": "automatic"
          },
          "ios": {
            "userInterfaceStyle": "automatic"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b70a5324-31cf-440d-bb9d-a89e1c47b694"
      }
    },
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "backgroundColor": "#000000"
    },
    "owner": "soft-glitch-solution"
  }
}
